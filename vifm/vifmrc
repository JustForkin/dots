only
filter /^Icon.*$|^~.*$|.*~$|^desktop.ini$/
colorscheme solarized
set viewcolumns=-{name},10{perms},8{size},16{mtime}
set timefmt=%y/%m/%d\ %H:%M
set wildmenu
set wildstyle=popup
set suggestoptions=normal,visual,view,otherpane,keys,marks,registers
set ignorecase
set smartcase
set incsearch
set statusline="  Hint: %z%= %A %10u:%-7g %15s %20d  "

mark 8 ~/github/diary/2018/ 08
mark a ~/rdlab/app/ greencard
mark b ~/github/physino/ tags/
mark c ~/rdlab/group/ detector\ characterization
mark d ~/Dropbox/ computer/
mark e ~/rdlab/teaching/Electrodynamics/18FA
mark f ~/rdlab/funding/ 19
mark g ~/github/ dots
mark h ~/ mail/
mark i ~/rdlab/isotope README.txt
mark j ~/rdlab/family/ Jing
mark l ~/rdlab/lab/ computer
mark m ~/rdlab/material/ scintillator
mark n ~/rdlab/experiment/NICE/ log
mark o ~/overleaf/
mark p ~/rdlab/gemadarc/ edu
mark t ~/rdlab/statistics README.txt
mark t ~/rdlab/ todo.md
mark u ~/rdlab/USD college
mark v ~/rdlab/video 18
mark w ~/rdlab/wisdom README.md

filetype *.sfs FUSE_MOUNT2|sshfs %PARAM %DESTINATION_DIR
filetype *.desktop gov %f &
filetype *.heprep hv %f &
filetype *.prim dawn %f 2> /dev/null &
filetype *.html,*.htm w3m %f
filetype *.o nm %f | less
" must be the last to cover things not covered above
" this will result in ignoring 'vi[x]cmd' option
" open should be listed last, 
" since the open cmd in Linux calls openvt instead of mimeopen
" mimeopen will ask if it cannot open some file
filetype * mimeopen,cygstart,open
"set vicmd='vim -X'

nnoremap S :sort<cr>
nnoremap s :!screen 1<cr>
nnoremap q :quit!<cr>

" vim: filetype=vim :
